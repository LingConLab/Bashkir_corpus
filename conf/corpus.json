{
  "corpus_name": "conlab_bashkir",
  "input_format": "json",
  "debug": false,
  "media": true,
  "media_length": 60,
  "max_context_expand": 6,
  "max_distance_filter": 200000,
  "max_docs_retrieve": 9999,
  "max_words_in_sentence": 40,
  "query_timeout": 60,
  "viewable_meta": [
    "filename",
    "title",
    "year",
    "speakers"
  ],
  "sentence_meta": [
    "speaker",
    "gender",
    "year_of_birth",
    "place_of_birth",
    "education",
    "is_able_to_write",
    "is_able_to_read"
  ],
  "search_meta": {
    "columns": [
      [
        {"type": "input_text", "name": "author", "placeholder": "Author"},
        {"type": "input_text", "name": "title", "placeholder": "Title"},
        {"type": "separator"},
        {"type": "input_number", "name": "year1", "placeholder": "Year: from"},
        {"type": "input_number", "name": "year2", "placeholder": "Year: to"}
      ],
      [
        {"type": "input_text", "name": "genre", "placeholder": "Genre"},
        {"type": "option", "name": "genre", "value": "interview", "text": "Interview"},
        {"type": "option", "name": "genre", "value": "monologue", "text": "Monologue"},
        {"type": "option", "name": "genre", "value": "experiment", "text": "Experiment"},
        {"type": "option", "name": "genre", "value": "interview|monologue|experiment", "text": "Any"}
      ]
    ],
    "stat_options": [
      "genre",
      "linguists",
      "speakers",
      "title",
      "year"
    ]
  },
  "word_fields":
  [
    "trans_en",
    "trans_ru"
  ],
  "languages": ["bashkir", "russian", "english"],
  "transliterations": ["original", "cyrillic"],
  "interface_languages": {"en": "English", "ru": "Russian"},
  "all_language_search_enabled": true,
  "gloss_search_enabled": true,
  "ambiguous_analyses": false,
  "lang_props":
  {
    "bashkir":
    {
      "gloss_shortcuts": {
        "case": "(unmarked|gen|abl|acc|dat|loc)(\\.(sg|pl))?",
        "poss": "(p\\.[123](sg|pl)?)",
        "tense": "(pst|prs|fut|evid)(\\.[123](sg|pl)?)?",
        "pst_any": "(pst(\\(1sg\\)|\\.[123](sg|pl))?)",
        "fut_any": "(fut(\\(1sg\\)|\\.[123](sg|pl))?)",
        "deriv": "(detr|iter|caus)",
        "pers": "[123]+(sg|pl)?"
      },
      "gr_fields_order": ["nType", "adjType", "proType", "stemType", "num_deriv", "v_form", "attributivizer", "tense", "person", "number", "case", "poss"],
      "gloss_selection": {
        "columns": [
          [
            {"type": "gloss", "value": "CASE", "tooltip": "Any&nbsp;case&nbsp;marker"},
            {"type": "gloss", "value": "Unmarked", "tooltip": "Nominative and unmarked accusative"},
            {"type": "gloss", "value": "ACC", "tooltip": "Marked accusative case"},
            {"type": "gloss", "value": "GEN", "tooltip": "Genitive case"},
            {"type": "gloss", "value": "ABL", "tooltip": "Ablative case"},
            {"type": "gloss", "value": "DAT", "tooltip": "Dative case"},
            {"type": "gloss", "value": "LOC", "tooltip": "Locative case"}
          ],
          [
            {"type": "gloss", "value": "PL", "tooltip": "Plural number"},
            {"type": "separator"},
            {"type": "gloss", "value": "POSS", "tooltip": "Any&nbsp;possessive&nbsp;marker"},
            {"type": "gloss", "value": "P.1SG", "tooltip": "1sg possessive"},
            {"type": "gloss", "value": "P.1PL", "tooltip": "1pl possessive"},
            {"type": "gloss", "value": "P.2SG", "tooltip": "2sg possessive"},
            {"type": "gloss", "value": "P.2PL", "tooltip": "2pl possessive"},
            {"type": "gloss", "value": "P.3", "tooltip": "3 singular or plural possessive"}
          ],
          [
            {"type": "gloss", "value": "ORD", "tooltip": "Ordinal numeral"},
            {"type": "gloss", "value": "DISTR", "tooltip": "Distributive numeral"},
            {"type": "gloss", "value": "COLL", "tooltip": "Collective numeral"},
            {"type": "gloss", "value": "APPROX", "tooltip": "Approximative"},
            {"type": "gloss", "value": "NUM.SUBST", "tooltip": "Numeral substantivizer"}
          ],
          [
            {"type": "gloss", "value": "DERIV", "tooltip": "Any verbal derivation"},
            {"type": "gloss", "value": "CAUS", "tooltip": "Causative"},
            {"type": "gloss", "value": "PASS", "tooltip": "Passive"},
            {"type": "separator"},
            {"type": "gloss", "value": "PST", "tooltip": "Past tense"},
            {"type": "separator"},
            {"type": "gloss", "value": "FUT", "tooltip": "Future tense"},
            {"type": "separator"},
            {"type": "gloss", "value": "NEG", "tooltip": "Negation"},
            {"type": "gloss", "value": "NEG.AUX", "tooltip": "Negative auxiliary"},
            {"type": "gloss", "value": "NEG.COP", "tooltip": "Negative copula"},
            {"type": "gloss", "value": "NEG.POT", "tooltip": "Potential negation"}
          ],
          [
            {"type": "gloss", "value": "PERS", "tooltip": "Any person marker"},
            {"type": "gloss", "value": "1SG", "tooltip": "1st person singular"},
            {"type": "gloss", "value": "1PL", "tooltip": "1st person plural"},
            {"type": "gloss", "value": "2SG", "tooltip": "2nd person singular"},
            {"type": "gloss", "value": "2PL", "tooltip": "2nd person plural"},
            {"type": "separator"},
            {"type": "gloss", "value": "Q", "tooltip": "Question marker"},
            {"type": "separator"},
            {"type": "gloss", "value": "AG", "tooltip": "Affix of agence formation"},
            {"type": "gloss", "value": "NMLZ", "tooltip": "Nominalization"},
            {"type": "gloss", "value": "POSS.SUBST", "tooltip": "Substantivizer of possessor"}
          ],
          [
            {"type": "gloss", "value": "CV", "tooltip": "General converb"},
            {"type": "gloss", "value": "CV.ANT", "tooltip": "Antecedence converb"},
            {"type": "gloss", "value": "CV.TERM", "tooltip": "Terminative converb"},
            {"type": "gloss", "value": "NEG.CV.ATT", "tooltip": "Antecedence negative converb"},
            {"type": "gloss", "value": "PTCP.FUT", "tooltip": "Future participle"},
            {"type": "gloss", "value": "PC.PST", "tooltip": "Past participle"}
          ]
        ]
      },
      "gramm_selection": {
        "columns": [
          [
            {"type": "header", "value": "Parts of speech"},
            {"type": "gramm", "value": "n", "category": "pos", "tooltip": "Noun"},
            {"type": "gramm", "value": "v", "category": "pos", "tooltip": "Verb"},
            {"type": "gramm", "value": "adj", "category": "pos", "tooltip": "Adjective"},
            {"type": "gramm", "value": "adv", "category": "pos", "tooltip": "Adverb"},
            {"type": "gramm", "value": "num", "category": "pos", "tooltip": "Numeral"},
            {"type": "gramm", "value": "pro", "category": "pos", "tooltip": "Pronoun"},
            {"type": "gramm", "value": "post", "category": "pos", "tooltip": "Postposition"},
            {"type": "gramm", "value": "cnj", "category": "pos", "tooltip": "Conjunction"},
            {"type": "gramm", "value": "part", "category": "pos", "tooltip": "Particle"},
            {"type": "gramm", "value": "intrj", "category": "pos", "tooltip": "Interjection"},
            {"type": "gramm", "value": "word", "category": "pos", "tooltip": "Not classified words"}
          ],
          [
            {"type": "header", "value": "Number"},
            {"type": "gramm", "value": "pl", "category": "number", "tooltip": "Plural"},
            {"type": "header", "value": "Adjective derivation"},
            {"type": "gramm", "value": "adj", "category": "adj_deriv", "tooltip": "Affix of adjective formation"},
            {"type": "gramm", "value": "car", "category": "adj_deriv", "tooltip": "Caritive"},
            {"type": "gramm", "value": "cmpr", "category": "adj_deriv", "tooltip": "Comparative"},
            {"type": "gramm", "value": "simil", "category": "adj_deriv", "tooltip": "Similative"},
            {"type": "gramm", "value": "intens", "category": "adj_deriv", "tooltip": "Intensive"},
            {"type": "header", "value": "Adverbial derivation"},
            {"type": "gramm", "value": "adv", "category": "adv_deriv", "tooltip": "Affix of adverbial formation"}
          ],
          [
            {"type": "header", "value": "Cases"},
            {"type": "gramm", "value": "unmarked", "category": "case", "tooltip": "Nominative or unmarked accusative"},
            {"type": "gramm", "value": "acc", "category": "case", "tooltip": "Accusative"},
            {"type": "gramm", "value": "gen", "category": "case", "tooltip": "Genitive"},
            {"type": "gramm", "value": "abl", "category": "case", "tooltip": "Ablative"},
            {"type": "gramm", "value": "dat", "category": "case", "tooltip": "Dative"},
            {"type": "gramm", "value": "loc", "category": "case", "tooltip": "Locative"}
          ],
          [
            {"type": "header", "value": "Possessiveness"},
            {"type": "gramm", "value": "p.1sg", "category": "poss", "tooltip": "1sg possessive"},
            {"type": "gramm", "value": "p.1pl", "category": "poss", "tooltip": "1pl possessive"},
            {"type": "gramm", "value": "p.2sg", "category": "poss", "tooltip": "2sg possessive"},
            {"type": "gramm", "value": "p.2pl", "category": "poss", "tooltip": "2pl possessive"},
            {"type": "gramm", "value": "p.3", "category": "poss", "tooltip": "3 possessive"}
          ],
          [
            {"type": "header", "value": "Tense/Mood"},
            {"type": "gramm", "value": "ipfv", "category": "tense", "tooltip": "Imperfective tense"},
            {"type": "gramm", "value": "fut", "category": "tense", "tooltip": "Future tense"},
            {"type": "gramm", "value": "pst", "category": "tense", "tooltip": "Past tense"},
            {"type": "gramm", "value": "plpf", "category": "tense", "tooltip": "Plusquamperfect"},
            {"type": "gramm", "value": "cond", "category": "mood", "tooltip": "Conditional"},
            {"type": "gramm", "value": "pot", "category": "mood", "tooltip": "Potential"},
            {"type": "gramm", "value": "desid", "category": "mood", "tooltip": "Desiderative"},
            {"type": "gramm", "value": "hort", "category": "mood", "tooltip": "Hortative"},
            {"type": "gramm", "value": "juss", "category": "mood", "tooltip": "Jussive"},
            {"type": "gramm", "value": "imp", "category": "mood", "tooltip": "Imperative"},
            {"type": "gramm", "value": "imp.emph", "category": "mood", "tooltip": "Emphatic imperative"}
          ],
          [
            {"type": "header", "value": "Verbal derivation"},
            {"type": "gramm", "value": "caus", "category": "tense", "tooltip": "Causative"},
            {"type": "gramm", "value": "pass", "category": "tense", "tooltip": "Passive"},
            {"type": "gramm", "value": "refl", "category": "tense", "tooltip": "Reflexive"},
            {"type": "gramm", "value": "recp", "category": "tense", "tooltip": "Recipient"}
          ]
        ]
      }
    }
  }
}
