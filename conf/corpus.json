        {"type": "separator"},
        {"type": "input_number", "name": "year_from", "placeholder": "Year: from"},
        {"type": "input_number", "name": "year_to", "placeholder": "Year: to"}
      ]
    ],
    "stat_options": [
      "speakers",
      "year",
      "title"
    ]
  },
  "word_fields":
